<!DOCTYPE html>
<html>
<head meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8" />    
    
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Para funcionar no Microsoft EDGE -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <link rel="stylesheet" href="css/bootstrap.css" />
    <link rel="stylesheet" href="css/bootstrap-responsive.css" />

    <title>@ViewBag.Title - SySDEA </title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="@Url.Content("~/Content/themes/base/jquery.ui.core.css")"
          rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")"
          rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.theme.css")"
          rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.ui.core.min.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.datepicker.min.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/DatePickerReady.js")"
            type="text/javascript"></script>
</head>
<body>

    <!-- <div class="navbar navbar-inverse navbar-fixed-top"> -->
    <div class="navbar-static-top navbar-inverse" style="margin-top:-50px">
        <div class"container">
            <!-- ************************************************************************************************* -->
            <div class"container " style="background:#1659bf; ">
                <div class="row-fluid">
                    <div class="container">

                        <div style="float: left; width: 30%; margin-top: 2%; margin-left: 7%">
                            <h style="color: white; font-size: 300%; face:Arial">
                                <span style="font-weight: bold">
                                    SySDEA <br />
                                </span>
                            </h>
                            <h style="color: white; font-size: 100%; face:Arial">
                                <div style="margin-top: -2%">
                                    Santos Dummont English Assessment System
                                </div>
                            </h>
                        </div>

                        <!-- ************************************************************************************************* -->
                        <div style="float: right; width: 35%; margin-top: 2%">
                            <h style="color: white; font-size: 300%; face:Arial">
                                <span style="font-weight: bold; ">
                                    ANAC  <br />
                                </span>
                            </h>

                            <h style="color: white; font-size: 100%; face:Arial">
                                <div style="margin-top: -2%">
                                    Agência Nacional de Aviação Civil
                                </div>
                            </h>
                        </div>

                        <div style="clear: both">
                        </div>

                    </div>
                    </br>
                    </br>
                </div>
            </div>
            <!-- ************************************************************************************************* -->
            <div class"container" style="background:#0f4098">

                <div class="row-fluid">
                    <div class="container">
                        <div class="masthead">

                            <div style="margin-top:-1%; margin-left:6%">

                                <div class="container">
                                    <ul class="nav navbar-nav navbar-left">

                                        <li>  @Html.ActionLink("Página inicial", "Index", "Home", null, new { @style = "color:#FFFFFF" })  </li>
                                        <li>  @Html.ActionLink("Sobre", "About", "Home", null, new { @style = "color:#FFFFFF" })           </li>
                                        <li>  @Html.ActionLink("Mapa do SySDEA", "Map", "Home", null, new { @style = "color:#FFFFFF" })    </li>
                                        <li>  @Html.ActionLink("Contato", "Contact", "Home", null, new { @style = "color:#FFFFFF" })       </li>

                                    </ul>



                                    <!--div class="navbar-right row-fluid" -->

                                    <div style="margin-left:12%">
                                        @Html.Partial("_LoginPartial")
                                    </div>
                                    <!---/div-->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ***************************************************************************************************** -->
    </div>

    <br />

    <div style="margin-left:10%">
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - PROFLING ANAC</p>
            </footer>
        </div>
    </div>
    @Scripts.Render("~bundles/CEP")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/inputmask")
    @RenderSection("scripts", required: false)
</body>
</html>
