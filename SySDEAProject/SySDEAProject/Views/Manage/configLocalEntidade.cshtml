 
@{
    ViewBag.Title = "Escolher filial";
}

<h2>Escolher Filial</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-group">
    @Html.Label("idLocalEntidade", "Filial", htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownList("idLocalEntidade", null, htmlAttributes: new { @class = "form-control" })
        
    </div>
</div>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Próximo" class="btn btn-default" />
    </div>
</div>
}